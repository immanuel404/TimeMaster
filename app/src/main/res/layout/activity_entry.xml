<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradientlist1"
    tools:context=".EntryActivity">

    <TextView
        android:id="@+id/lblCategory"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:layout_marginTop="479dp"
        android:text="Category:"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblPay"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginTop="585dp"
        android:text="Pay rate:"
        android:textColor="@color/purple_500"
        android:textSize="19sp"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblTax"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_marginTop="585dp"
        android:text="Tax rate:"
        android:textColor="@color/purple_500"
        android:textSize="19sp"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblEarning"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="524dp"
        android:text="Earnings ($):"
        android:textSize="19sp"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblEnd"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:layout_marginTop="344dp"
        android:text="End:"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgLogo"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imgEntry"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="136dp"
        android:src="@drawable/clock"
        android:visibility="visible"
        app:civ_border_color="#000"
        app:civ_border_width="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblDate"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:layout_marginTop="253dp"
        android:text="Date:"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblStart"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:layout_marginTop="298dp"
        android:text="Start:"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblDescription"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:layout_marginTop="434dp"
        android:text="Description:"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblMinMax"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:layout_marginTop="389dp"
        android:text="Min:   Max:"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblHrsWorked"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="524dp"
        android:text="Hrs Worked:"
        android:textSize="19sp"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDelete"
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:layout_marginTop="74dp"
        android:background="@drawable/round_button"
        android:text="Delete"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:backgroundTint="#EF2323"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnBack"
        android:layout_width="250dp"
        android:layout_height="46dp"
        android:layout_marginBottom="31dp"
        android:background="@drawable/round_button"
        android:text="Back"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="2dp"
        android:layout_height="45dp"
        android:text="TextView"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="524dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="2dp"
        android:layout_height="45dp"
        android:text="TextView"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="588dp" />

</androidx.constraintlayout.widget.ConstraintLayout>